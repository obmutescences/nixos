{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#83C092",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },

        {
          "foreground": "#9ACE65",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_icon": "\uf126 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "#DBBC7F",
          "style": "plain",
          "template": "<#A8BAC5>is</> \uf487 {{.Version}} ",
          "type": "project"
        },
        {
          "foreground": "#D699B6",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#A8BAC5>via</> \ue718 {{ .Full }} ",
          "type": "node"
        },
        {
          "foreground": "#E69875",
          "style": "plain",
          "template": "<#A8BAC5>and</> \ue71e {{.Full}} ",
          "type": "npm"
        },
        {
          "foreground": "#7FBBB3",
          "style": "plain",
          "template": "<#A8BAC5>via</> \ue63c {{.Full}} ",
          "type": "python",
          "properties": {
            "fetch_version": true,
            "fetch_virtual_env": true
          }
        },
        {
          "foreground": "#A7C080",
          "style": "plain",
          "template": "<#A8BAC5>via</> \ue626 {{.Full}} ",
          "type": "go",
          "properties": {
            "fetch_version": true
          }
        },
        {
          "foreground": "#E69875",
          "style": "plain",
          "template": "<#A8BAC5>via</> \ue7a8 {{.Full}} ",
          "type": "rust",
          "properties": {
            "fetch_version": true
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#DBBC7F",
          "style": "plain",
          "template": "\u276f ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 3
}
